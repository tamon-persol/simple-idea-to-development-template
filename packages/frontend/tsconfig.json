{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "composite": true,
    "incremental": true,
    "target": "ES2015",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "preserve",
    "outDir": "./dist",
    "allowJs": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": false
  },
  "references": [
    {
      "path": "../model/tsconfig.json"
    }
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
